{
  "name": "use-api-ts",
  "version": "0.0.0",
  "description": "React dev friendly and type-safe hook for your api",
  "main": "./lib/src/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib",
    "coverage": "codecov",
    "lint": "eslint ./src",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladi-strilets/use-api-ts.git"
  },
  "license": "MIT",
  "author": {
    "name": "Vladimir Strilets",
    "email": "vladi-strilets@users.noreply.github.com",
    "url": "https://github.com/vladi-strilets"
  },
  "engines": {
    "node": ">=14.0"
  },
  "keywords": [
    "boilerplate",
    "template",
    "typescript",
    "vscode",
    "jest",
    "husky",
    "commitizen",
    "semantic-release",
    "codecov"
  ],
  "bugs": {
    "url": "https://github.com/vladi-strilets/use-api-ts/issues"
  },
  "homepage": "https://github.com/vladi-strilets/use-api-ts#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "master"
    ]
  }
}
